.tabbar {
  position: absolute;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  bottom: 0;
  width: 100%;
  text-align: center;
  font-size: 14px;
  align-items: center;
  background-color: #fbfbfb;
  border-top: 1rpx solid #b2b2b2;
  z-index: 10;
}

.tabbar-item {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: center;
  height: 100rpx;
}

.tabbar-new {
  position: absolute;
  left: 50%;
  margin-left: -46rpx;
  top: 50%;
  margin-top: -46rpx;
  flex: none;
  height: 92rpx;
  width: 92rpx;
  border-radius: 92rpx;
  background-color: #0076ff;
  z-index: 5;
  transition: all .5s;
}

.tabbar-new_selected {
  box-shadow: 0 0 20px #b2b2b2;
  transform: rotate(45deg);
}

.icon-new {
  font-size: 30px!important;
  color: #fff;
  font-weight: lighter;
}